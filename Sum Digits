using System;
using System.Globalization;

namespace SoftUni
{
    class Program
    {
        static void Main(string[] args)
        {


            int number = int.Parse(Console.ReadLine());
            string numberT = number.ToString();
            int sum = 0;

            for(int i=0;i<numberT.Length;i++)
            {
                char digit = numberT[i];
                double numberValue = char.GetNumericValue(digit);
                sum += (int)numberValue;


                


            }

            Console.WriteLine(sum);



        }
    }
}

